G_CONST_RETURN G@Type@Value *
@enum_name@_from_name (const char *name)
{
  const G@Type@Value *v = &@enum_name@_values[0];

  g_return_val_if_fail(NULL != name, NULL);

  while(v->value_name)
  {
    if (!strcmp(v->value_name, name))
      return v;

    v++;
  }

  return NULL;
}

